
    <section class="section-header">
        <div class="header__hero">
            <div class="header__hero-overlay">&nbsp;</div><img class="header__hero-img"
                src="<%= details.imgUrl %>" alt="The Forest Hiker">
        </div>
        <div class="heading-box">
            <h1 class="heading-primary"><span><%-details.name%></span></h1>
            <div class="heading-box__group">
                <div class="heading-box__detail"><svg class="heading-box__icon">
                        <use xlink:href="/img/icons.svg#icon-clock"></use>
                    </svg><span class="heading-box__text"><%=details.duration%></span></div>
                <div class="heading-box__detail"><svg class="heading-box__icon">
                        <use xlink:href="/img/icons.svg#icon-map-pin"></use>
                    </svg><span class="heading-box__text"><%-details.location%></span></div>
            </div>
        </div>
    </section>
    <section class="section-description">
        <div class="overview-box">
            <div>
                <div class="overview-box__group">
                    <h2 class="heading-secondary ma-bt-lg">Quick facts</h2>
                    <div class="overview-box__detail"><svg class="overview-box__icon">
                            <use xlink:href="/img/icons.svg#icon-calendar"></use>
                        </svg><span class="overview-box__label">Next date</span><span class="overview-box__text"><%-details.startDates%></span></div>
                    <div class="overview-box__detail"><svg class="overview-box__icon">
                            <use xlink:href="/img/icons.svg#icon-trending-up"></use>
                        </svg><span class="overview-box__label">Difficulty</span><span
                            class="overview-box__text"><%-details.difficulty%></span></div>
                    <div class="overview-box__detail"><svg class="overview-box__icon">
                            <use xlink:href="/img/icons.svg#icon-user"></use>
                        </svg><span class="overview-box__label">Participants</span><span class="overview-box__text"><%-details.maxGroupSize%>
                            people</span></div>
                    <div class="overview-box__detail"><svg class="overview-box__icon">
                            <use xlink:href="/img/icons.svg#icon-star"></use>
                        </svg><span class="overview-box__label">Rating</span><span class="overview-box__text"><%-details.rating%> /
                            5</span></div>
                </div>
                <div class="overview-box__group">
                    <h2 class="heading-secondary ma-bt-lg">Your tour guides</h2>
                    <div class="overview-box__detail"><img class="overview-box__img"
                            src="/img/users/user-5c8a1f292f8fb814b56fa184-1574777070682.jpeg"
                            alt="Steve T. Scaife"><span class="overview-box__label">Lead guide</span><span
                            class="overview-box__text">Steve T. Scaife</span></div>
                    <div class="overview-box__detail"><img class="overview-box__img" src="/img/users/user-7.jpg"
                            alt="Kate Morrison"><span class="overview-box__label">Tour guide</span><span
                            class="overview-box__text">Kate Morrison</span></div>
                    <div class="overview-box__detail"><img class="overview-box__img"
                            src="/img/users/user-5c8a1f292f8fb814b56fa184-1574777070682.jpeg"
                            alt="Leo J. Gillespie"><span class="overview-box__label">Tour guide</span><span
                            class="overview-box__text">Leo J. Gillespie</span></div>
                </div>
            </div>
        </div>
        <div class="description-box">
            <h2 class="heading-secondary ma-bt-lg">About The <%-details.name%></h2>
            <p class="description__text"><%-details.description%></p>

        </div>
    </section>
    <section class="section-pictures">
        <div class="picture-box"><img class="picture-box__img picture-box__img--1" src="/img/tours/tour-1-1.jpg"
                alt="The Park Camper Tour 1"></div>
        <div class="picture-box"><img class="picture-box__img picture-box__img--2" src="/img/tours/tour-1-2.jpg"
